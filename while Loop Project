using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace whileLoopIterationProject
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 0;
            int b = 4;

            while(a<b)
            {
                Console.WriteLine("Condition is True : " + a);
                a++;  // Incriment of a variable
            }
            Console.Read();
        }
    }
}
