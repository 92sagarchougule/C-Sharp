using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Array
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] esat = new string [5];    //  Array Creation 
            esat[0] = "Gangu";                //  Array Values
            esat[1] = "Mamta";
            esat[2] = "Sagar";
            esat[3] = "Values";
            esat[4] = "Mohmad";

            Console.WriteLine("value of esat is:" +esat[2]);
            Console.Read();
        }
    }
}
