using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ArrayProject
{
    class Program
    {
        static void Main(string[] args)
        {

            string[] Names = new string[4];

            Names[0] = "Jayshri";
            Names[1] = "Dhanappa";
            Names[2] = "Sagar";
            Names[3] = "Jaya";

            for (int i = 0; i <4; i++)
            {
               Console.WriteLine("The Array Value is: "+ Names[i]);
            }


            Console.WriteLine("Length of Array : " + Names.Length);


            Array.Resize(ref Names, 10);

            Console.WriteLine("Length of Array After Resize is : " + Names.Length);


            Console.Read();
        }
    }
}
