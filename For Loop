using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace forLoopIterationProject
{
    class Program
    {
        static void Main(string[] args)
        {

            for (int i=0;i<=5;i++)  // Condition will always true (;;)   // (int i; ; ) // (int i=0; i<=5;)
            {
                Console.WriteLine("Hi");
            }
            Console.Read();
        }
    }
}
